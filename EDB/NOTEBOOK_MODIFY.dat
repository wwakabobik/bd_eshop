, P_ID = :P_ID
, TYPE = :TYPE
, NAME = :NAME
, CPU = :CPU
, MEMORY = :MEMORY
, HDD = :HDD
, DISPLAY = :DISPLAY
, VIDEO = :VIDEO
, AUDIO = :AUDIO
, DRIVE = :DRIVE
, PORTS = :PORTS
, NETWORK = :NETWORK
, WEIGHT = :WEIGHT
, SIZES = :SIZES
, MANUFACTURER = :MANUFACTURER
, PIC_NOTE = :PIC_NOTE
where
ID = :OLD_ID
