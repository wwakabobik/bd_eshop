update CUSTOMERS
set
CUSTOMER_ID = :CUSTOMER_ID
, FAMILY_NAME = :FAMILY_NAME
, FIRST_NAME = :FIRST_NAME
, LAST_NAME = :LAST_NAME
, ADRESS = :ADRESS
, PHONE = :PHONE
, EMAIL = :EMAIL
, OFFERS_TOTAL = :OFFERS_TOTAL
, OFFERS_NOW = :OFFERS_NOW
, VIP = :VIP
where
CUSTOMER_ID = :OLD_CUSTOMER_ID
